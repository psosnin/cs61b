public class Triangles {
    public static void main(String[] args) {
       String star = "*";
       int count = 1;
       while (count < 10) {
          int num = count;
          while (num > 0) {
             System.out.print(star);
                num = num - 1;
          }
          System.out.println("");
          count = count + 1; 
       }
    }
}